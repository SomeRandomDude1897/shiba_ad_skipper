{
    "manifest_version": 3,
    "name": "SHIBA AD SKIPPER 9000",
    "version": "0.1",
    "description": "Skips ads on youtube. Youtube blocks adblockers, so I created this.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  